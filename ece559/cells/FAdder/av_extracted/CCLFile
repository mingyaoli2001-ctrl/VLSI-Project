	 -merge_parallel_res false \
	 -min_res 0.001
input_db -type pvs \
	 -design_cell_name "FAdder layout tutorial" \
	 -directory_name "/home/ecegridfs/a/559mg81/pvs/svdb" \
	 -format "DFII" \
	 -library_definitions_file "/home/ecegridfs/a/559mg81/cds.lib" \
	 -run_name "FAdder"
log_file \
	 -file_name "/home/ecegridfs/a/559mg81/pvs/svdb/qrc.FAdder.log"
output_db -type extracted_view \
	 -cap_component "pcapacitor" \
	 -cap_property_name "c" \
	 -cdl_out_map_directory \
		"/home/ecegridfs/a/559mg81/pvs" \
	 -device_finger_delimiter "@" \
	 -enable_cellview_check true \
	 -include_cap_model "false" \
	 -include_parasitic_cap_model "false" \
	 -include_parasitic_res_model "comment" \
	 -include_parasitic_res_width true \
	 -include_res_model "false" \
	 -res_component "presistor" \
	 -res_property_name "r" \
	 -transfer_net_expression true \
	 -view_name "av_extracted"
output_setup \
	 -directory_name "/home/ecegridfs/a/559mg81/pvs/svdb" \
	 -temporary_directory_name "FAdder"
process_technology \
	 -technology_corner \
		"rcx_typical" \
